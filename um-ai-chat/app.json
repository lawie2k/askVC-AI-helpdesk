{
  "expo": {
    "name": "um-ai-chat",
    "slug": "um-ai-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ios-light.png",
    "backgroundColor": "#900C27",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/ios-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#900C27"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.um-ai-chat",
      "splash": {
        "image": "./assets/ios-light.png",
        "resizeMode": "contain",
        "backgroundColor": "#900C27"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ios-light.png",
        "backgroundColor": "#900C27"
      },
      "edgeToEdgeEnabled": true,
      "splash": {
        "image": "./assets/ios-light.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#900C27"
      },
      "package": "com.lawie.umaichat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fe6efd5b-135c-4203-9110-dabdc8d27dfa"
      }
    },
    "owner": "lawie"
  }
}
